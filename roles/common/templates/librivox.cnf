# LV fine tuning

[mysqld]

skip_name_resolve
wait_timeout = 60
max_connections = 200
query_cache_type = 0
query_cache_size = 0
table_open_cache = 750
tmp_table_size = 32M
max_heap_table_size = 32M
innodb_buffer_pool_size = {{ innodb_buffer_pool_size }}
innodb_buffer_pool_instances = 6
innodb_file_per_table
innodb_log_file_size = 750M
innodb_flush_log_at_trx_commit = 2
